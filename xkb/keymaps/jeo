default xkb_keymap "ansi" {
    xkb_keycodes  { include "evdev+aliases(qwerty)"       };
    xkb_types     { include "complete"                    };
    xkb_compat    { include "complete"                    };
    xkb_symbols   { include "pc+jeo(ansi)+inet(evdev)"    };
    xkb_geometry  { include "pc(pc105)"                   };
};

xkb_keymap "iso" {
    xkb_keycodes  { include "evdev+aliases(qwerty)"       };
    xkb_types     { include "complete"                    };
    xkb_compat    { include "complete"                    };
    xkb_symbols   { include "pc+jeo(iso)+inet(evdev)"     };
    xkb_geometry  { include "pc(pc105)"                   };
};

xkb_keymap "kinesis" {
    xkb_keycodes  { include "evdev+aliases(qwerty)"       };
    xkb_types     { include "complete"                    };
    xkb_compat    { include "complete"                    };
    xkb_symbols   { include "pc+jeo(kinesis)+inet(evdev)" };
    xkb_geometry  { include "pc(pc105)"                   };
};
