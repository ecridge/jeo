default xkb_keymap "iso-desktop" {
    xkb_keycodes  { include "evdev+aliases(qwerty)"                 };
    xkb_types     { include "complete+jeo"                          };
    xkb_compat    { include "complete+jeo"                          };
    xkb_symbols   { include "pc+inet(evdev)+jeo"                    };
    xkb_geometry  { include "pc(pc105)"                             };
};

